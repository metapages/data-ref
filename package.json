{
  "name": "metaframe-template",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "prebuild": "npm i && ./node_modules/typescript/bin/tsc --build --verbose",
    "build": "rm -rf dist/* && node_modules/parcel-bundler/bin/cli.js build public/index.html --no-autoinstall"
  },
  "dependencies": {
    "@metapages/metapage": "^0.5.7",
    "isomorphic-fetch": "^3.0.0",
    "parcel-bundler": "^1.12.5",
    "preact": "^10.5.13",
    "preact-router": "^3.2.1",
    "query-string": "^6.14.1",
    "react": "^16.8.0",
    "react-router-dom": "^5.2.0",
    "typescript": "4.1.5",
    "use-query-params": "^1.2.1"
  },
  "browserslist": {
    "production": [
      "last 3 and_chr versions",
      "last 3 chrome versions",
      "last 3 opera versions",
      "last 3 ios_saf versions",
      "last 3 safari versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
